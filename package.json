{
  "name": "flug-portal",
  "version": "1.0.0",
  "description": "Example Project",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:aot": "ng build --aot",
    "build:aot-german": "ng build --aot --i18n-file src/locale/messages.de.xlf --locale de --i18n-format xlf",
    "build:all": "npm run build && npm run build:prod && npm run build:aot && npm run build:aot-german",
    "ng": "ng",
    "start": "ng serve --port 8081",
    "start:no-animation": "ng serve --env no-animation",
    "test": "ng test",
    "test:single": "ng test --single-run true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:start": "ng e2e --port 4200 --env no-animation",
    "translate": "ng xi18n",
    "check": "npm run test:single && npm run e2e:start && npm run translate && npm run build:all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@angular/upgrade": "^4.2.4",
    "@ngx-translate/core": "7.0.0",
    "@ngx-translate/http-loader": "0.1.0",
    "angular-oauth2-oidc": "1.0.20",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "jquery": "3.2.1",
    "rxjs": "5.4.1",
    "web-animations-js": "2.2.5",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.2.7",
    "@angular/compiler-cli": "^4.2.4",
    "@types/jasmine": "2.5.42",
    "@types/node": "8.0.3",
    "codelyzer": "3.1.1",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "mkdirp": "0.5.1",
    "protractor": "5.1.2",
    "ts-node": "3.1.0",
    "tslint": "5.4.3",
    "typescript": "2.3.4"
  }
}
